extends layout

block content
  h1 #{title}
  p Hostname #{hostname}
  p IP Address #{ip}
  p URL Host #{host}
  p User Agent #{agent}

  h2 PUG テスト画面
  ul
    each val, index in ['zero', 'one', 'two']
      li= index + ': ' + val

  
  label.checkbox
  input.checkbox-input(type='checkbox')
  span.checkbox-label チェックボックステスト

  h2 アプリの更新

